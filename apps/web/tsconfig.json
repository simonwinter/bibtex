{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "module": "preserve",
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "esnext",
    "verbatimModuleSyntax": true,
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "esModuleInterop": true,
  },
  "include": ["src"]
}