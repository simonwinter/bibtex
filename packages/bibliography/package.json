{
  "name": "@df-astro/bibliography",
  "version": "0.0.1",
  "type": "module",
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node ./dist/index.js",
    "bib": "npm run build && npm run start",
    "clean": "npm run rimraf ./src/content/bibliography/*.json; npm run rimraf ./dist",
    "bib:download": "./bin/dev.js bibtex download -o ./test.json --no-colour"
  },
  "exports": {
    "./io": {
      "import": "./dist/io.js"
    },
    "./doiAPI": {
      "import": "./dist/doiAPI.js"
    },
    "./handlers": {
      "import": "./dist/handlers.js"
    },
    "./log": {
      "import": "./dist/log.js"
    },
    "./schema": {
      "import": "./dist/schema.js"
    },
    "./utils": {
      "import": "./dist/utils.js"
    }
  },
  "dependencies": {
    "@orcid/bibtex-parse-js": "0.0.25",
    "@types/node": "20.14.8",
    "biblatex-csl-converter": "2.1.0",
    "typescript": "5.4.5",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@oclif/core": "^4.0.12",
    "@total-typescript/tsconfig": "1.0.4",
    "ansi-colors": "4.1.3",
    "chalk": "5.3.0",
    "cli-progress": "3.12.0",
    "log-symbols": "6.0.0",
    "loglevel": "1.9.1",
    "oclif": "4.14.5",
    "ora": "8.0.1",
    "rimraf": "5.0.7",
    "ts-node": "10.9.2"
  },
  "bin": {
    "bibliography": "./bin/run.js"
  },
  "oclif": {
    "bin": "bibliography",
    "commands": "./dist/commands",
    "dirname": "bibliography",
    "topicSeparator": " ",
    "nodeOptions": [
      "--experimental-json-modules"
    ]
  }
}
